; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "Grid",
"language": "GDScript",
"path": "res://Grid/Grid.gd"
}, {
"base": "Node2D",
"class": "Level",
"language": "GDScript",
"path": "res://Level/Level.gd"
}, {
"base": "Control",
"class": "LevelGUI",
"language": "GDScript",
"path": "res://LevelGUI/LevelGUI.gd"
}, {
"base": "Node",
"class": "SceneManager",
"language": "GDScript",
"path": "res://SceneManager.gd"
}, {
"base": "Area2D",
"class": "Tile",
"language": "GDScript",
"path": "res://Tile/Tile.gd"
}, {
"base": "Resource",
"class": "TileData",
"language": "GDScript",
"path": "res://Tile/TileData.gd"
} ]
_global_script_class_icons={
"Grid": "",
"Level": "",
"LevelGUI": "",
"SceneManager": "",
"Tile": "",
"TileData": ""
}

[application]

config/name="Agroecology"
run/main_scene="res://Start/startScene.tscn"
config/icon="res://icon.png"

[autoload]

sceneManager="*res://SceneManager.gd"
BackgroundMusic="*res://BackgroundMusic.tscn"

[display]

window/stretch/mode="2d"
window/stretch/aspect="keep"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
